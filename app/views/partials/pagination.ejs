<% if (typeof pages !== 'undefined' && typeof page !== 'undefined' && typeof query !== 'undefined') { %>
        <% if (pages > 0) { %>
          <% let q = '' %>
            <% for (var key in query) { %>
          <% if (query.hasOwnProperty(key) && key != 'page') q += query.key %>
          <% } %>
    <nav aria-label="...">
      <ul class="pagination justify-content-end">
        <% if (page == 1) { %>
        <li class=" page-item disabled"><a class="page-link">previous</a></li>
        <% } else { %>
        <li class="page-item "><a class="page-link" href="?page=<%= page - 1 %>&<%= q%>">previous</a></li>
        <% } %>
        <% var i = (Number(page) > 5 ? Number(page) - 4 : 1) %>
        <% if (i !== 1) { %>
        <li class="  page-item disabled"><a>...</a></li>
        <% } %>
        <% for (; i <= (Number(page) + 4) && i <= pages; i++) { %>
        <% if (i == page) { %>
        <li class=" page-item active "><a class="page-link"><%= i %></a></li>
        <% } else { %>
        <li class=" page-item  "><a class="page-link" href="?page=<%= i %>&<%= q%>"><%= i %></a></li>
        <% } %>
        <% if (i == Number(page) + 4 && i < pages) { %>
        <li class="page-item disabled"><a>...</a></li>
        <% } %>
        <% } %>
        <% if (page == pages) { %>
        <li class="page-item disabled"><a class="page-link">Last</a></li>
        <% } else { %>
        <li class=" page-item  "><a class="page-link" href="?page=<%= pages %>&<%= q%>">Last</a></li>
        <% } %>
      </ul>
    </nav>
    <% } }%>