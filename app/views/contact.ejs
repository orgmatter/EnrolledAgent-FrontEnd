      <div class="main-content">
        <!-- Section: inner-header -->
        <section class="inner-header divider layer-overlay overlay-dark-4 contact_jumbotron">
          <div class="container pt-120 pb-60">
            <!-- Section Content -->
            <div class="section-content">
              <div class="row">
                <div class="col-md-6">
                  <h2 class="text-theme-colored2 font-36">Contact</h2>
                </div>
              </div>
            </div>
          </div>
        </section>

        <!-- Divider: Google Map -->
        <section>
          <div class="container-fluid pt-0 pb-0">
            <div class="row">
              <!-- Google Map HTML Codes -->
              <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3024.213334355686!2d-74.01230048459024!3d40.713319945471774!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c25a1922b97571%3A0x32d71fbaca083c5f!2s100%20Church%20St%2C%20New%20York%2C%20NY%2010007%2C%20USA!5e0!3m2!1sen!2sng!4v1611348506554!5m2!1sen!2sng" 
              width="100%" height="450" frameborder="0" style="border:0;" allowfullscreen aria-hidden="false" tabindex="0"></iframe>
            </div>
          </div>
        </section>

        <!-- Divider: Contact -->
        <section class="divider">
          <div class="container">
            <div class="row pt-30">
              <div class="col-md-4">
                <div class="row">
                  <div class="col-xs-12 col-sm-12 col-md-12">
                    <div class="icon-box left media bg-deep p-30 mb-50">
                      <a class="media-left pull-left" href="#">
                        <i class="pe-7s-map-2 text-theme-colored"></i
                      ></a>
                      <div class="media-body">
                        <h5 class="mt-0">Our Office Location</h5>
                        <p>100 Church St, New York, NY 10007</p>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-12 col-sm-6 col-md-12">
                    <div class="icon-box left media bg-deep p-30 mb-50">
                      <a class="media-left pull-left" href="#">
                        <i class="pe-7s-call text-theme-colored"></i
                      ></a>
                      <div class="media-body">
                        <h5 class="mt-0">Contact Number</h5>
                        <p> 1-855-22-AGENT</p>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-12 col-sm-6 col-md-12">
                    <div class="icon-box left media bg-deep p-30 mb-50">
                      <a class="media-left pull-left" href="#">
                        <i class="pe-7s-mail text-theme-colored"></i
                      ></a>
                      <div class="media-body">
                        <h5 class="mt-0">Email Address</h5>
                        <p>info@enrolledagent.com</p>
                      </div>
                    </div>
                  </div>
                  <!-- <div class="col-xs-12 col-sm-6 col-md-12">
                    <div class="icon-box left media bg-deep p-30 mb-20">
                      <a class="media-left pull-left" href="#">
                        <i class="pe-7s-film text-theme-colored"></i
                      ></a>
                      <div class="media-body">
                        <h5 class="mt-0">Make a Video Call</h5>
                        <p>Enrolled Agent</p>
                      </div>
                    </div>
                  </div> -->
                </div>
              </div>
              <div class="col-md-8">
                <h3 class="line-bottom mt-0 mb-30">Interested in discussing?</h3>
                <!-- Contact Form -->
                <form id="contact-form" name="contact_form" class="">
                  <div class="row">
                    <div class="col-sm-12">
                      <div class="form-group">
                        <label>Name <small>*</small></label>
                        <input
                          name="form_name"
                          class="form-control"
                          type="text"
                          placeholder="Full Name" 
                          id="fullName" 
                          required 
                        />
                      </div>
                    </div>
                    <div class="col-sm-12">
                      <div class="form-group">
                        <label>Email <small>*</small></label>
                        <input
                          name="form_email"
                          class="form-control required email"
                          type="email"
                          placeholder="Enter Email"
                          id="email" 
                          required
                        />
                      </div>
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Subject <small>*</small></label>
                        <input
                          name="form_subject"
                          class="form-control required"
                          type="text"
                          placeholder="Enter Subject"
                          id="subject" 
                          required
                        />
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Phone<small>*</small></label>
                        <input
                          name="form_phone"
                          class="form-control"
                          type="tel"
                          placeholder="Enter Phone Number"
                          id="phone" 
                          required
                        />
                      </div>
                    </div>
                  </div>

                  <div class="form-group">
                    <label>Message<small>*</small></label>
                    <textarea
                      name="form_message"
                      class="form-control required"
                      rows="5"
                      placeholder="Enter Message"
                      id="message" 
                      required
                    ></textarea>
                  </div>
                  <div class="form-group">
                    <input name="form_botcheck" class="form-control" type="hidden" value="" />
                    <button
                      type="submit"
                      class="btn btn-dark btn-theme-colored btn-flat mr-5"
                      id="submit-btn"
                    >
                      Send your message
                    </button>
                    <button type="reset" class="btn btn-default btn-flat btn-theme-colored">
                      Reset
                    </button>
                  </div>
                </form>

