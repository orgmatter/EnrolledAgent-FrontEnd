{
  "name": "enrolled_agents",
  "version": "0.0.1",
  "description": "An agent listing application",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "babel ./server -d dist",
    "start": "npm run build && node index.js",
    "sass": "node-sass  --output-style compressed sass/stylesheets/style.scss -o ../public/stylesheets/",
    "watch:sass": "npm run sass && npm run sass -- -w",
    "postdev": "npm run sass",
    "dev": "concurrently \"npm run watch:sass\" \"DEBUG=server,api,email-templates nodemon --ignore public,node-modules,upload\"",
    "dev-win": "concurrently \"npm run watch:sass\" \"set DEBUG=server,api,email-templates && nodemon --ignore public,node-modules,upload\"",
    "lint": "eslint "
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bamyx-it-solutions/enrolled-agents.git"
  },
  "author": "Wisdom Ekeh",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/bamyx-it-solutions/enrolled-agents/issues"
  },
  "homepage": "https://gitlab.com/bamyx-it-solutions/enrolled-agents#readme",
  "dependencies": {
    "async": "^3.2.0",
    "chalk": "^4.1.0",
    "common": "file:../common",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "email-templates": "^7.0.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-useragent": "^1.0.13",
    "helmet": "^3.23.3",
    "mongoose": "^5.9.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "nodemailer-mailgun-transport": "^2.0.0",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2",
    "uid": "^1.0.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
